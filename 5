line2 = ["â¬œï¸","â¬œï¸","ï¸â¬œï¸"]
line3 = ["â¬œï¸ï¸","â¬œï¸ï¸","â¬œï¸ï¸"]
map = [line1, line2, line3]
print("Hiding your treasure! X marks the spot.")
position = input() # Where do you want to put the treasure?

# map[0][0] = "A1"
# map[0][1] = "B1"
# map[0][2] = "C1"

# map[1][0] = "A2"
# map[1][1] = "B2"
# map[1][2] = "C2"

# map[2][0] = "A3"
# map[2][1] = "B3"
# map[2][2] = "C3"

if position == "A1":
  map[0][0] = "X"
elif position == "B1":
  map[0][1] = "X"
elif position == "C1":
  map[0][2] = "X"
elif position == "A2":
  map[1][0] = "X"
elif position == "B2":
  map[1][1] = "X"
elif position == "C2":
  map[1][2] = "X"
elif position == "A3":
  map[2][0] = "X"
elif position == "B3":
  map[2][1] = "X"
elif position == "C3":
  map[2][2] = "X"
else:
  print("error")












